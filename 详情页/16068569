<script type="text/javascript">
	//转发至手机
	$("#sentToMp").bind('click', function() {
		$.qfangDialog.DialogShow("sentToMphone");
	});
	$("#sentToWx").bind('click', function() {
		$.qfangDialog.DialogShow("sentToWeixin");
	});
    $("#sentToMphoneClose").on('click', function() {
      $.qfangDialog.DialogClose("sentToMphone");
    });
    
	$("#sentToWeixinClose").bind('click', function() {
		$.qfangDialog.DialogClose("sentToWeixin");
	});
	//刷新转发到手机验证码 
	function changeVoliCode () {
	 $("#sentToMphoneCodeImg").attr("src",ctx+'/checkCode/getValidateCode?r='+Math.random());
	}

	//短信转发
	$("#sendPhoneFormSubmit").click(function() {
		var phone=$("#ftransmitPhone").val();
		var valiCode=$("#valiCode").val();
		reg = /^[1][3578]\d{9}$/;
		if($.trim(phone)=="") {
			$("#phoneError").removeClass("dn");
			$("#phoneErrorText").html("请输入手机号码");
			 return;
		}
		if (phone.length !== 11 || !reg.test(phone)) {
			$("#phoneError").removeClass("dn");
			 $('#phoneErrorText').html('手机号码输入错误！');
	        return;
	    }
		if($.trim(valiCode)=="") {
			$("#codeError").removeClass("dn");
			$("#codeErrorText").html("请输入验证码");
			return ;
		}
		var url = ctx+'/detail/sendPhoneTransmit';
		if (typeof(roomType) != "undefined" && roomType == 'BUILDING') {
			url = ctx+'/office/detail/sendPhoneTransmit';
		}
		$.ajax( {    
            url:url,// 跳转到 action    
            type:'post',
            data:$('#sendPhoneForm').serialize(), 
            dataType:'json',    
            success:function(data) {
            	var object=jQuery.parseJSON(data);
            	if(data.flag=="1") {
            		$("#codeError").removeClass("dn");
            		$("#codeErrorText").html("验证码输入有误");
            	}
            	if(data.flag=="2") {
            		$("#phoneError").removeClass("dn");
            		$("#phoneErrorText").html("同一手机号码每日不能超过十次");
            	}
            	if(data.flag=="3") {
            		$("#phoneError").addClass("dn");
            		$("#phoneErrorText").html("");
            		$("#codeError").addClass("dn");
            		$("#codeErrorText").html("");
            		$.qfangAlertTips.Okay("转发成功", '3000');
            		window.location.reload();
            	}
            	if(data.flag=="4") {
            		$("#phoneError").removeClass("dn");
            		$("#phoneErrorText").html("系统繁忙");
            	}
             }  
        });
	});
</script>
<!-- end 转发 -->
	
