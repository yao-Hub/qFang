<script type="text/javascript">
	$(function(){
		//更多学校
		$('body').on('click', '#moreSchool', function() {
			$("div[key='moreSchool']").show();
			$(this).hide();
		});
	})
	
	var rommcommentId = rcId;
	$(document).ready(function(){
		rommcommentClick(rommcommentId,refererURL);
		
		 // 房源对比最近浏览
		var oldLooks = $.cookie('looks');
		//var newLooks = 'SALE,'+"55598"+','+"55598";
		var newLooks = 'SALE,'+rommcommentId+','+"55598";
		if (oldLooks) {
			var looksArr = oldLooks.split('|');
			var newLooksArr = [];
			
			
			for (var i = 0; i < looksArr.length; i++) {
				if (newLooks != looksArr[i]) {
					newLooksArr.push(looksArr[i]);
				}
			}
			newLooksArr.push(newLooks);
			if (newLooksArr.length > 3) {
				newLooksArr = newLooksArr.slice(-3);
			} 
			newLooks = newLooksArr.join('|');
		}
		$.cookie('looks', newLooks, {expires : 365*10, path : '/'});
		
	});	
	
</script>
</body>
