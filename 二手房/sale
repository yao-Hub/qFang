<script src="/themes/default/scripts/frontend/common/echarts/echarts-2.2.7/echarts.js?qv=1491966349796"></script>
<script src="/themes/default/scripts/frontend/FrontendEcharts.js?qv=1491966349796"></script>
<script src="/themes/default/scripts/frontend/common/jquery.md5.js?qv=1491966349796"></script>
<script>
	
	
	$(function(){
		
		var keyword = '';
		(
			function(){
				if(keyword){
					keyword = keyword.replace(/\(/g,'');
					keyword = keyword.replace(/\)/g,'');
					var showKeywords = $("[key='showKeyword']");
					if(keyword.indexOf(" ") != -1){
						var keywords = $.trim(keyword).split(" ");
						for(var j=0;j<keywords.length;j++){
							keyword = keywords[j];
							if(keyword ==  "" || keyword ==  " ")
								continue;
							for(var i=0;i<showKeywords.length;i++){
								$(showKeywords[i]).html($(showKeywords[i]).html().replace(new RegExp(keyword, 'g'),'<em class="high-light">'+keyword+'</em>'));
							}
						}
					}else{
						for(var i=0;i<showKeywords.length;i++){
							$(showKeywords[i]).html($(showKeywords[i]).html().replace(new RegExp(keyword, 'g'),'<em class="high-light">'+keyword+'</em>'));
						}
					}
				}
			}
		)();
		
	  	//$.qfangAdChange("#zhuhai,#adFullWidth");
	    var $functionsBar = $('#functionsBar');
	    var $cycleListings = $('#cycleListings');
	    var $loading = $('#loading');
	    var $loadingInset = $('#loadingInset');
	    var $loadingOFT = $loading.offset().top;
	    var flagAjax = true; //控制加载完就不再执行加载操作了
	    
	    
	    //滚动加载房源列表
	    
	    $(window).on('scroll', function() {
	      var winST = $(window).scrollTop() + $(window).height();
	      var currentPage=1;
	      if (winST >= $loadingOFT && flagAjax  && currentPage<=1) {
	        $loading.find('img').show();

	        flagAjax = false;
			var pageUrl='/sale/ajax/f2';
	        //alert(pageUrl)
	        
	        
	        //加载房源
			$.ajax({
				url:pageUrl,
				type:"post",
				success:function(data) {
					$('#pagesBox').remove();
			        //房源列表输出
			        $cycleListings.find('ul').append(data);
			        //房源加载完后执行
			        $loading.find('img').hide();
			        $loading.siblings('.btns_turnpage').removeClass('dn');
				}
			});
			

	      }

	    });
	    
	});
</script>
